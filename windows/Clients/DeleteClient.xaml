        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rentPrac1.windows.Clients"
        mc:Ignorable="d"
        Title="DeleteClient" Height="250" Width="350">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black"/>
                <GradientStop Color="#FFBF3A3A" Offset="0.891"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label Content="ID" HorizontalAlignment="Left" Margin="35,43,0,0" VerticalAlignment="Top" Foreground="#FFCEFF00" FontSize="20"/>
        <TextBox x:Name="IdInput" HorizontalAlignment="Left" Margin="97,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="165" Background="#FFE89494" FontSize="20"/>
        <Button Content="Сохранить" HorizontalAlignment="Left" Margin="40,131,0,0" VerticalAlignment="Top" Width="117" Click="Button_Click_1" Foreground="#FFCEFF00" Background="#FFE89494" FontSize="20"/>
