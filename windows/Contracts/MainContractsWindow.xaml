<Window x:Class="rentPrac1.windows.Contracts.MainContractsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rentPrac1.windows.Contracts"
        mc:Ignorable="d"
        Title="MainContractsWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,76,0,0" SelectionChanged="dataList_SelectionChanged" MouseEnter="dataList_MouseEnter"/>
        <Button Content="Добавить контракт" HorizontalAlignment="Left" Margin="24,27,0,0" VerticalAlignment="Top" Width="131" Click="Button_Click"/>
        <Button Content="Выход" HorizontalAlignment="Left" Margin="698,32,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
