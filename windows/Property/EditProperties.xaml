<Window x:Class="rentPrac1.windows.Property.EditProperties"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rentPrac1.windows.Property"
        mc:Ignorable="d"
        Title="EditProperties" Height="450" Width="800">
    <Grid>
        <Label Content="Name" HorizontalAlignment="Left" Margin="35,39,0,0" VerticalAlignment="Top" FontSize="22"/>
        <TextBox x:Name="nameinput" HorizontalAlignment="Left" Margin="153,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="226"/>
        <Label Content="Address" HorizontalAlignment="Left" Margin="25,93,0,0" VerticalAlignment="Top" FontSize="22"/>
        <TextBox x:Name="addressInput" HorizontalAlignment="Left" Margin="153,104,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="226"/>
        <Label Content="Price" HorizontalAlignment="Left" Margin="40,160,0,0" VerticalAlignment="Top" FontSize="22"/>
        <TextBox x:Name="priceInput" HorizontalAlignment="Left" Margin="153,171,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="226" PreviewTextInput="priceInput_PreviewTextInput"/>
        <Button Content="Сохранить" HorizontalAlignment="Left" Margin="29,262,0,0" VerticalAlignment="Top" Height="38" Width="173" Click="Button_Click"/>
        <Button Content="Удалить" HorizontalAlignment="Left" Margin="244,262,0,0" VerticalAlignment="Top" Height="38" Width="173" Click="Button_Click_1"/>
        <Button Content="Отмена" HorizontalAlignment="Left" Margin="459,262,0,0" VerticalAlignment="Top" Height="38" Width="173" Click="Button_Click_2"/>

    </Grid>
</Window>
